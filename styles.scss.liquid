  }
  
  .vertical-menu .sublink:hover {
    
    .icon-right-arrow::before {
      animation: rotate-menu-arrow 0.1s ease-in-out forwards;
    }
    
    .vertical-menu_submenu li a {
      position: relative;
      transition: .2s ease;
    }
    
    .vertical-menu_submenu li:nth-child(1) a {
      animation: fade-menu 0.1s ease forwards;
    }
    .vertical-menu_submenu li:nth-child(2) a {
      animation: fade-menu 0.2s ease forwards;
    }
    .vertical-menu_submenu li:nth-child(3) a {
      animation: fade-menu 0.3s ease forwards;
    }
    .vertical-menu_submenu li:nth-child(4) a {
      animation: fade-menu 0.4s ease forwards;
    }
    .vertical-menu_submenu li:nth-child(5) a {
      animation: fade-menu 0.5s ease forwards;
    }
    .vertical-menu_submenu li:nth-child(6) a {
      animation: fade-menu 0.6s ease forwards;
    }
    .vertical-menu_submenu li:nth-child(7) a {
      animation: fade-menu 0.7s ease forwards;
    }
    .vertical-menu_submenu li:nth-child(8) a {
      animation: fade-menu 0.8s ease forwards;
    }
    .vertical-menu_submenu li:nth-child(9) a {
      animation: fade-menu 0.9s ease forwards;
    }
    .vertical-menu_submenu li:nth-child(10) a {
      animation: fade-menu 1s ease forwards;
    }
  }
}

@keyframes rotate-menu-arrow {
  0% {
    transform: rotate(0deg);
  }
  25% { transform: rotate(30deg);
  }
  50% { transform: rotate(60deg);
  }
  100% { transform: rotate(90deg);
  }
}

@keyframes fade-menu {
  0% { 
    opacity: 0;
    transform: translateY(-50px);
  }
  100% { 
    opacity: 1;
    transform: translateY(0px);
  }
}

