.caption {
  width: 100%;
  position: absolute;
  {% if settings.header_background %}
    top: 50%;
  {% else %}
    top: 55%;
  {% endif %}
  padding: 0 5%;
  @include prefix(transform, translate(0,-50%), ms webkit spec);
  @include respond-to('medium-up') {
    max-width: 960px;
    left: 0;
    right: 0;
    margin: 0 auto;
  }
  @include respond-to('large-up') {
    padding: 0 30px;
  }
  @include respond-to('medium-down') {
    width: 100%;
  }
  @include respond-to('small-down') {
    margin: 20px auto;
    font-size: 3vw;
    position: static;
    transform: none;
    width: 100%;
  }
  a {
    pointer-events: all;
  }
  &.position-center {
    left: 0;
    right: 0;
    text-align: center;
    margin-left: auto;
    margin-right: auto;
    @include respond-to('medium-down') {
      width: 85%;
    }
    @include respond-to('small-down') {
      width: 100%;
    }
  }
  &.position-left {
